<body>

    <div id="wrapper">

        <!-- Sidebar -->
        <div id="sidebar-wrapper">
            <ul class="sidebar-nav">
                <li class="sidebar-brand">
                    <a href="#">
                        QFS Fantasy Football
                    </a>
                </li>
                <li>
                  {{#link-to "owners"}}Owners Dashboard{{/link-to}}
                </li>
                <li>
                  {{#link-to "power-ranks"}}Rankings{{/link-to}}
                </li>
            </ul>
        </div>
        <!-- /#sidebar-wrapper -->

        <!-- Page Content -->
        <div id="page-content-wrapper">
          <div class='container'>
            <a href="#menu-toggle" class="btn btn-secondary" id="menu-toggle">{{fa-icon "bars" size=2 ariaLabel="Click Me"}}</a>
            {{outlet}}

          </div>
        </div>
        <!-- /#page-content-wrapper -->

    </div>
    <!-- /#wrapper -->

    <!-- Bootstrap core JavaScript -->
    <script src="vendor/jquery/jquery.min.js"></script>
    <script src="vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Menu Toggle Script -->
    <script>
    $("#menu-toggle").click(function(e) {
        e.preventDefault();
        $("#wrapper").toggleClass("toggled");
    });
    </script>

</body>
